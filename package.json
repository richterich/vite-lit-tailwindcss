{
  "name": "vite-lit-tailwindcss",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint:check": "eslint \"**/*.js\" --debug",
    "lint:write": "eslint \"**/*.js\" --fix",
    "prettier:check": "prettier -c **/*.{js,html,css,json} --log-level \"debug\"",
    "prettier:write": "prettier -w **/*.{js,html,css,json}",
    "list:staged": "lint-staged",
    "prepare": "husky install"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.15",
    "eslint": "^8.47.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "postcss": "^8.4.28",
    "prettier": "^3.0.2",
    "prettier-plugin-tailwindcss": "^0.5.3",
    "tailwindcss": "^3.3.3",
    "vite": "^4.4.9"
  },
  "dependencies": {
    "lit": "^2.8.0"
  },
  "browserslist": [
    ">0.5%",
    "last 3 versions",
    "supports es6-module",
    "supports css-grid",
    "not dead"
  ]
}
